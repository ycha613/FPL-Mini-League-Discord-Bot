{
  "name": "fpl-mini-league-discord-bot",
  "version": "1.0.0",
  "description": "Discord bot that provides updates on transfers, point totals, and captains for a mini league.",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "clean": "del /s /q dist",
    "build": "tsc --dist",
    "start": "npm run clean && npm run build && node dist/index.js",
    "startNoClean": "npm run build && node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycha613/FPL-Mini-League-Discord-Bot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/ycha613/FPL-Mini-League-Discord-Bot/issues"
  },
  "homepage": "https://github.com/ycha613/FPL-Mini-League-Discord-Bot#readme",
  "dependencies": {
    "axios": "^1.13.6",
    "discord.js": "^14.25.1",
    "dotenv": "^17.3.1"
  },
  "devDependencies": {
    "@types/node": "^25.3.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
